<?xml version="1.0"?>
<Template>
	<TemplateConfiguration>
		<_Name>Form (with form preview)</_Name>
		<Icon>md-xml-file-icon</Icon>
		<_Category>Eto.Forms</_Category>
		<LanguageName>F#</LanguageName>
		<_Description><![CDATA[Creates a new Eto.Forms cross platform form class using F#.

This template provides a partial *.eto.fs file for form preview.]]>
		</_Description>
		<DefaultFilename>MyForm</DefaultFilename>
		<GroupId>eto-file-panel</GroupId>
	</TemplateConfiguration>
	<TemplateFiles>
        <File name="${Name}.fs" AddStandardHeader="true" src="preview.panel.fs"/>
        <File name="${Name}.eto.fs" AddStandardHeader="true" src="preview.form.eto.fs" DependsOn="${Name}.fs" />
	</TemplateFiles>
</Template>
